    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "lerna": "^5.3.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.77.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
