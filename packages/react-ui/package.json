{
  "name": "@appbuckets/react-ui",
  "version": "2.0.0-alpha.1",
  "description": "Just Another React UI Framework",
  "keywords": [
    "react",
    "react-component",
    "appbuckets",
    "style",
    "components"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Marco Cavanna",
    "email": "marco@appbuckets.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcocavanna/appbuckets.git",
    "directory": "packages/react-bucket"
  },
  "bugs": {
    "url": "https://github.com/marcocavanna/appbuckets/issues"
  },
  "homepage": "https://react-bucket.appbuckets.io",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config ../../rollup.config.js --input src/index.ts",
    "build:types": "tsc -p tsconfig.declaration.json",
    "prebuild": "rimraf build tsconfig.declaration.tsbuildinfo && yarn eslint",
    "postbuild": "yarn build:types && yarn prettier",
    "eslint": "eslint ./src",
    "prettier": "prettier --config ../../.prettierrc --write \"build/**/*{.js,.ts}\""
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@appbuckets/react-ui-core": "1.0.0-alpha.1"
  }
}
