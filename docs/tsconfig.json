{
  "extends": "../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "src",
    "pages"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "preserve"
  }
}
